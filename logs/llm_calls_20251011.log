                      {formatPrice(item.price * item.quantity)}
                    </Text>
                  </VStack>
                  <HStack>
                    <Button
                      size="sm"
                      onClick={() => updateQuantity(item.id, item.quantity - 1)}
                    >
                      -
                    </Button>
                    <Text>{item.quantity}</Text>
                    <Button
                      size="sm"
                      onClick={() => updateQuantity(item.id, item.quantity + 1)}
                    >
                      +
                    </Button>
                  </HStack>
                  <Button
                    colorScheme="red"
                    variant="ghost"
                    onClick={() => removeFromCart(item.id)}
                  >
                    <FaTrash />
                  </Button>
                </HStack>
                <Divider my={4} />
              </Box>
            ))}

            <Box pt={4}>
